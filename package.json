{
  "name": "ostltd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/scss/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "deploy": "gh-pages -d build -m \"Automatic build\"",
    "start": "gulp start"
  },
  "browserslist": [
    "> 0.5%",
    "last 4 versions",
    "Firefox ESR",
    "not dead"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.scss"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "autoprefixer": "^9.8.6",
    "browser-sync": "^2.26.12",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "gh-pages": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "scss-resets": "^0.5.2",
    "gulp-imagemin": "^7.1.0",
    "imagemin-mozjpeg": "^8.0.0",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-modernizr": "^3.4.1",
    "gulp-webp": "^4.0.1",
    "gulp-csso": "^3.0.1",
    "gulp-svgstore": "^7.0.1",
    "gulp-posthtml": "^3.0.4",
    "posthtml-include": "^1.2.0",
    "node-normalize-scss": "^8.0.0",
    "normalize-scss": "^7.0.1",
    "imagemin-pngquant": "^8.0.0"
  },
  "dependencies": {
    "swiper": "latest",
    "@glidejs/glide": "^3.4.1",
    "lodash": "^4.17.19"
  }
}
